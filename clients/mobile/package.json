    "babel-plugin-import": "^1.13.0",
    "es6-promise": "^4.2.8",
    "fastclick": "^1.0.6",
    "lint-staged": "^10.0.7",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "rc-form": "^2.4.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "store2": "^2.11.0",
    "umi": "^3.0.16",
    "yorkie": "^2.0.0"
  }
