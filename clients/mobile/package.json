{
  "private": true,
  "version": "1.0.0",
  "name": "minapp-client-mobile",
  "author": "WangWei",
  "description": "A demo code for minapp.",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
